{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "allowImportExportEverywhere": true
    },
    "rules": {
        "prettier/prettier": "error"
    }
}
